<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.sibusiso.mamelodisundowns"
        versionCode = "1.0" 
        version     = "1.0.0" >

		<!-- versionCode is optional and Android only -->

		<name>weep</name>

		<description>
			Developed and Design by Sibusiso Mthimunye 
		</description>

		<author href="https://build.phonegap.com" email="sibusiso_ppsd@yahoo.com">
			Sibusiso Mthimunye
		</author>

		
		<feature name="SocialSharing">
			<param name="android-package" value="nl.xservices.plugins.SocialSharing" />
		</feature>

		
		<feature name="SocialSharing">
			<param name="wp-package" value="SocialSharing"/>
			
		</feature>
		

		
		<icon src="picl.jpg" gap:platform="android" gap:qualifier="ldpi" />
		<icon src="pich.png" gap:platform="android" gap:qualifier="mdpi" />
		<icon src="pich.png" gap:platform="android" gap:qualifier="hdpi" />
		<icon src="pich.png" gap:platform="android" gap:qualifier="xhdpi" />
		<icon src="pich.png" gap:platform="android" gap:qualifier="xxhdpi" />

		<gap:platform name="ios" />
		<gap:platform name="android" />
		<gap:platform name="winphone" />
		
		<feature name="debug-server" required="true">
			<param name="domain" value="http://debug.custom.com"/>
			<param name="key" value="some_unique_key"/>
		</feature>

		<gap:splash src="icon/splash.jpg" />
		<gap:splash src="icon/splash1.jpg" width="320" height="480" />
		<gap:splash src="icon/splash2.jpg" width="640" height="960" />
		<gap:splash src="icon/splash3.jpg" width="768" height="1024" />
		<plugin name="cordova-plugin-battery-status" spec="~2.0.3" />
<plugin name="cordova-plugin-camera" spec="~4.1.0" />
<plugin name="cordova-plugin-console" spec="~1.1.0" />
<plugin name="cordova-plugin-contacts" spec="~3.0.1" />
<plugin name="cordova-plugin-device" spec="~2.0.3" />
<plugin name="cordova-plugin-device-motion" spec="~2.0.1" />
<plugin name="cordova-plugin-device-orientation" spec="~2.0.1" />
<plugin name="cordova-plugin-dialogs" spec="~2.0.2" />
<plugin name="cordova-plugin-file" spec="~6.0.2" />
<plugin name="cordova-plugin-file-transfer" spec="~1.7.1" />
<plugin name="cordova-plugin-geolocation" spec="~4.0.2" />
<plugin name="cordova-plugin-globalization" spec="~1.11.0" />
<plugin name="cordova-plugin-inappbrowser" spec="~3.1.0" />
<plugin name="cordova-plugin-media" spec="~5.0.3" />
<plugin name="cordova-plugin-media-capture" spec="~3.0.3" />
<plugin name="cordova-plugin-network-information" spec="~2.0.2" />
<plugin name="cordova-plugin-splashscreen" spec="~5.0.3" />
